@model IEnumerable<WorkingWithVS.Models.Product>
@{ Layout = null;}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Working with Visual Studio</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <td>Name</td><td>Price</td>
            </tr>
        </thead>
        <tbody>
            @foreach(var a in Model)
            {
                <tr>
                    <td>@a.Name</td>
                    <td>@a.Price</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>

